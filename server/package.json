{
    "name": "server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "test": "TMPDIR=./.vitest_tmp vitest",
        "test:run": "TMPDIR=./.vitest_tmp vitest run",
        "seed": "TMPDIR=./.vitest_tmp ts-node-dev --transpile-only src/scripts/seed.ts"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.39.8",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "openai": "^4.29.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.25",
        "@types/supertest": "^6.0.3",
        "supertest": "^7.2.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.4.2",
        "vitest": "^4.0.18"
    }
}