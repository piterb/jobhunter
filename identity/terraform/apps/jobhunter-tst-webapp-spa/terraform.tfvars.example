# Copy to terraform.tfvars for LOCAL usage only.
# This file holds sensitive credentials. Do NOT commit terraform.tfvars.
#
# In CI, values are provided from GH vars/secrets with KEYCLOAK_* prefix.
keycloak_url           = "https://keycloak.example.com"
keycloak_client_id     = "terraform-identity-jobhunter-tst-webapp-spa"
keycloak_client_secret = "<set-local-secret>"
realm_name             = "<set-existing-realm-name>"
environment            = "tst"

jobhunter_tst_webapp_spa_spa_redirect_uris = [
  "http://localhost:3000/auth/callback"
]

jobhunter_tst_webapp_spa_spa_web_origins = [
  "http://localhost:3000"
]

jobhunter_tst_webapp_spa_api_audience = "jobhunter-tst-webapp-spa-api"

jobhunter_tst_webapp_spa_enable_direct_access_grants = false
